{
  "name": "fisikaseru",
  "version": "1.0.0",
  "description": "Fisikaseru â€“ Interactive Physics Labs (Milikan Rocket)",
  "main": "index.html",
  "private": true,
  "license": "MIT",
  "author": "",
  "scripts": {
    "build:css": "npx @dhiwise/component-tagger && npx tailwindcss -i ./css/tailwind.css -o ./css/main.css",
    "watch:css": "npx @dhiwise/component-tagger && npx tailwindcss -i ./css/tailwind.css -o ./css/main.css --watch",
    "build:css:clean": "npx tailwindcss -i ./css/tailwind.css -o ./css/main.css",
    "watch:css:clean": "npx tailwindcss -i ./css/tailwind.css -o ./css/main.css --watch",
    "serve": "node ./server/index.js",
    "dev": "npm run watch:css",
    "dev:all": "concurrently \"npm run watch:css\" \"npm run serve\"",
    "dev:all:clean": "concurrently \"npm run watch:css:clean\" \"npm run serve\"",
    "dev:full": "start cmd /c \"npm run serve\" & npm run watch:css",
    "dev:labs": "npx http-server . -p 5174 -c-1",
    "dev:platform:server": "node ./platform/server/index.js",
    "dev:platform:client": "npm --prefix ./platform/client run dev"
  },
  "dependencies": {
    "@dhiwise/component-tagger": "^1.0.14",
    "@tailwindcss/forms": "^0.5.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-elevation": "^2.0.0",
    "tailwindcss-fluid-type": "^2.0.7"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "dotenv": "^16.6.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.17",
    "http-server": "^14.1.1"
  }
}
